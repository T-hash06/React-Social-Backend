// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

model User{
	id       String @id @default(uuid())
	name     String
	username String @unique
	email    String @unique
	password String
}

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
